/* program that prompts user to enter telephone number in form (xxx) xxx-xxxx and then displays number as xxx.xxx.xxxx */

#include <stdio.h>

int main(void)
{
    int num1, num2, num3;

    printf("Enter phone number [(xxx) xxx-xxxx]: ");
    scanf("(%d) %d-%d", &num1, &num2, &num3);
    printf("You entered %d.%d.%d", num1, num2, num3);

    return 0;
}

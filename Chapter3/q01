/* Program that accepts date from user in form mm/dd/yyyy and displays it in form yyyymmdd */

#include <stdio.h>

int main(void)
{
    int mm, dd, yyyy;

    printf("Enter a date (mm/dd/yyyy): ");
    scanf("%d/%d/%d", &mm, &dd, &yyyy);
    printf("You entered the date %d%0.2d%0.2d", yyyy, mm, dd);

    return 0;
}
